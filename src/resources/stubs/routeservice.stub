
    /**
     *
     * Graphview generated route file.
     *
     * Define the "graph" routes for the application.
     *
     * These routes all receive session state, CSRF protection, etc.
     *
     * @return void
     */
    protected function mapGraphRoutes()
    {
        Route::middleware('web')
             ->namespace('Bespired\Graphview\Http\Controllers')
             ->group(base_path('routes/graphview.php'));
    }

